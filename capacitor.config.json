{
  "appId": "co.magicwork.app",
  "appName": "magicwork",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https",
    "iosScheme": "https",
    "hostname": "magicwork.vercel.app",
    "allowNavigation": [
      "https://magicwork.vercel.app",
      "https://*.vercel.app",
      "https://*.supabase.co"
    ]
  },
  "ios": {
    "contentInset": "automatic",
    "scrollEnabled": true,
    "backgroundColor": "#fcf8f2"
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#fcf8f2",
    "buildOptions": {
      "keystorePath": "",
      "keystorePassword": "",
      "keystoreAlias": "",
      "keystoreAliasPassword": ""
    }
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#fcf8f2",
      "showSpinner": false
    }
  }
}

